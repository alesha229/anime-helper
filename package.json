{
  "name": "anime-helper",
  "displayName": "anime-helper",
  "description": "anime-helper",
  "icon": "icon.png",
  "version": "0.0.4",
  "publisher": "anime-helper",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-extension-samples/issues"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "anime.show",
        "title": "Аниме: Показать модель"
      }
    ]
  },
  "activationEvents": [
    "onCommand:anime.show"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "cowsay": "^1.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^22",
    "@types/vscode": "^1.100.0",
    "eslint": "^9.13.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
